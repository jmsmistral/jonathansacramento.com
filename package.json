{
  "name": "jonathansacramento.com",
  "version": "0.1.0",
  "description": "Jonathan Sacramento Personal Website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmsmistral/jonathansacramento.com"
  },
  "homepage": "https://jmsmistral.github.io/jonathansacramento.com",
  "main": "app.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src/**/*.js ./test/**/*.js",
    "build:client:dev": "./node_modules/.bin/webpack",
    "build:client:prod": "./node_modules/.bin/webpack -p --env production",
    "build-babel:client": "./node_modules/.bin/babel ./src/client/app.js --out-dir ./public/dist/",
    "test:client": "./node_modules/.bin/jest ./tests/client/ --collectCoverageFrom=**/tests/client/**/*.test.js",
    "test:server": "./node_modules/.bin/mocha --require @babel/register ./tests/server/**/*.test.js",
    "dev-server:client": "./node_modules/.bin/webpack-dev-server",
    "dev-server:client:docker": "./node_modules/.bin/webpack-dev-server --host 0.0.0.0",
    "start:client": "DEBUG=app* nodemon ./src/client/server.js",
    "build:blog": "node ./src/client/utils/mdToHtmlConverter.js",
    "predeploy": "npm run build:blog && npm run build:client:prod",
    "deploy": "./node_modules/gh-pages/bin/gh-pages.js -d ./public"
  },
  "keywords": [
    "react",
    "personal",
    "web",
    "blog",
    "code"
  ],
  "author": "Jonathan Sacramento",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-brands-svg-icons": "5.9.0",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "babel-loader": "8.0.6",
    "body-parser": "1.18.3",
    "bootstrap": "4.5.0",
    "chalk": "2.4.1",
    "css-loader": "2.1.1",
    "debug": "4.1.1",
    "express": "4.16.4",
    "gh-pages": "^2.1.1",
    "highlightjs": "^9.12.0",
    "jquery": "^3.5.1",
    "marked": "1.1.0",
    "mini-css-extract-plugin": "0.7.0",
    "morgan": "1.9.1",
    "node-sass": "4.14.1",
    "nodemon": "1.18.9",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-bootstrap": "1.0.1",
    "react-dom": "16.13.1",
    "react-modal": "3.11.2",
    "react-reveal": "1.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.32.2",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "5.11.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-html": "6.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "jest": "24.8.0",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "sinon": "7.3.2",
    "webpack-dev-server": "^3.11.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 3000,
      "VIEWS_DIR": "views"
    }
  }
}
